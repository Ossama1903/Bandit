{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2020",
    "outDir": "dist",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "bin/**/*",
  ]
}
